{
  "name": "demo-depart",
  "version": "0.3.3-alpha.0",
  "description": "Orchestrated compiles, links, and deploys for an Ethereum distributed country",
  "keywords": [
    "ethereum",
    "orchestration",
    "compile",
    "link",
    "deploy",
    "javascript"
  ],
  "author": "Paul Pham <paul@invisible.college>",
  "homepage": "https://github.com/invisible-college/democracy/tree/master/packages/depart#readme",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "./index.js",
  "directories": {
    "src": "src",
    "tests": "tests"
  },
  "bin": {
    "demo-depart": "./bin/demo-depart",
    "demo-test": "NODE_ENV=DEVELOPMENT ./bin/demo-test --departFileName departDS.js"
  },
  "files": [
    "src",
    "index.js",
    "bin/demo-depart",
    "bin/demo-test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/invisible-college/democracy.git"
  },
  "scripts": {
    "test:mocha": "NODE_ENV=DEVELOPMENT mocha ./tests -t 25000 --trace-warnings --exit --colors --recursive --reporter spec",
    "test:flow": "NODE_ENV=DEVELOPMENT ./bin/demo-test --mochaTimeout 10000 --unlockSeconds 20",
    "test": "yarn test:mocha && yarn test:flow",
    "cov": "nyc.js mocha tests/",
    "console": "NODE_ENV=DEVELOPMENT ./bin/demo-console"
  },
  "bugs": {
    "url": "https://github.com/invisible-college/democracy/issues"
  },
  "dependencies": {
    "demo-client": "^0.3.1-alpha.4",
    "demo-compile": "^0.3.3-alpha.0",
    "demo-contract": "^0.3.3-alpha.0",
    "demo-keys": "^0.3.3-alpha.0",
    "demo-utils": "^0.3.1-alpha.4",
    "immutable": "4.0.0-rc.12"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "demo-test-contracts": "^0.2.7-alpha.7"
  },
  "gitHead": "e6eb5b4ddb1a324c68d19be66a52b2c3f77adb6c"
}
